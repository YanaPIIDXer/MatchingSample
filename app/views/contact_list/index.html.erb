<%= render partial: "application/pageheader", locals: {pageTitle: "コンタクト一覧"} %>

<% if !@contactUsers.empty? %>
    <% @contactUsers.each do |user| %>
        <%= link_to user["name"], "/contact?user_id=#{user["user_id"]}" %><br />
    <% end %>
<% else %>
    コンタクトはありません。
<% end %>

<br />
